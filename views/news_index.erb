<% @stories.each do |story| %>	

<div><a href="<%=story.link%>" target ="_blank"><h3><%=story.title%></h3></a></div>


<a href="/hacknews/<%= story.id %>/edit">EDIT</a>

<div class = 'buttons' ><form action="/hacknews/<%= story.id %>/delete" method="post">
	<input type="submit" name="Delete" value="delete" />
</form></div>

<div><a href="/hacknews/new/link">Share your new link!</a></div>
<div><a href="/hacknews/new/text">Share your new story!</a></div>

<% end %>













